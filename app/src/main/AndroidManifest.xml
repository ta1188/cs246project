<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cs246project.kindergartenprepapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LetterTraceableActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".NumberTraceableActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".NameTraceableActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".SightWordTraceableActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".WordSelectable"
            android:screenOrientation="landscape" />
        <activity
            android:name=".NameSelectable"
            android:screenOrientation="landscape" />
        <activity
            android:name=".CountSelectable"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MenuActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".AboutActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ShapeSelectable"
            android:screenOrientation="landscape" >
        </activity>
    </application>

</manifest>